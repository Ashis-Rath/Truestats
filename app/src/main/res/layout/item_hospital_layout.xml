<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginBottom="10dp"
    android:adjustViewBounds="false"
    android:paddingBottom="10dp">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="261dp"
        android:background="@drawable/box"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nameOfOrgCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/segoeuibold"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Name of Organisation"
        android:textColor="#264157"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/addressOfOrgCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Address of Organisation"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/nameOfOrgCovTV"
        app:layout_constraintVertical_bias="0.033" />

    <View
        android:id="@+id/view2"
        android:layout_width="0.5dp"
        android:layout_height="150dp"
        android:background="#707070"
        app:layout_constraintBottom_toTopOf="@+id/updateDateTimeCovTV"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.637"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cityOfOrgCovTV" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="163dp"
        android:fontFamily="@font/sfprodisplayregular"
        android:text="ICU/Ventilator Beds:"
        android:textColor="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.27"
        app:layout_constraintStart_toStartOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="112dp"
        android:fontFamily="@font/sfprodisplayregular"
        android:text="Oxygen Beds:"
        android:textColor="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView14"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="61dp"
        android:fontFamily="@font/sfprodisplayregular"
        android:text="Normal Beds:"
        android:textColor="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView15"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/totalVentiBedCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/total_bed_colour"
        android:fontFamily="@font/calibriregular"
        android:text="Total: "
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.091"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:layout_constraintVertical_bias="0.057" />

    <TextView
        android:id="@+id/totalOxyBedCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/total_bed_colour"
        android:fontFamily="@font/calibriregular"
        android:text="Total: "
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/totalVentiBedCovTV"
        app:layout_constraintStart_toStartOf="@+id/totalVentiBedCovTV"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        app:layout_constraintVertical_bias="0.087" />

    <TextView
        android:id="@+id/totalNormalBedTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/total_bed_colour"
        android:fontFamily="@font/calibriregular"
        android:text="Total: "
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/totalOxyBedCovTV"
        app:layout_constraintStart_toStartOf="@+id/totalOxyBedCovTV"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        app:layout_constraintVertical_bias="0.208" />

    <TextView
        android:id="@+id/cityOfOrgCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="City"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/textView14"
        app:layout_constraintEnd_toEndOf="@+id/addressOfOrgCovTV"
        app:layout_constraintStart_toStartOf="@+id/addressOfOrgCovTV"
        app:layout_constraintTop_toBottomOf="@+id/addressOfOrgCovTV" />

    <TextView
        android:id="@+id/vacantVentiBedCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/calibriregular"
        android:text="Vacant: "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/totalVentiBedCovTV"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.439"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/totalVentiBedCovTV"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/vacantOxyBedCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/calibriregular"
        android:text="Vacant: "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/totalOxyBedCovTV"
        app:layout_constraintEnd_toEndOf="@+id/vacantVentiBedCovTV"
        app:layout_constraintStart_toStartOf="@+id/vacantVentiBedCovTV"
        app:layout_constraintTop_toTopOf="@+id/totalOxyBedCovTV" />

    <TextView
        android:id="@+id/vacantNormalBedTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/calibriregular"
        android:text="Vacant: "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/totalNormalBedTV"
        app:layout_constraintEnd_toEndOf="@+id/vacantOxyBedCovTV"
        app:layout_constraintStart_toStartOf="@+id/vacantOxyBedCovTV"
        app:layout_constraintTop_toTopOf="@+id/totalNormalBedTV"
        app:layout_constraintVertical_bias="0.187" />

    <TextView
        android:id="@+id/updateDateTimeCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/box_date_time"
        android:fontFamily="@font/calibribold"
        android:text="Last Updated: "
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:textColor="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/vacantNormalBedTV" />

    <TextView
        android:id="@+id/callBtnCov"
        android:layout_width="71dp"
        android:layout_height="28dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/button_colour"
        android:clickable="true"
        android:drawablePadding="0dp"
        android:gravity="center_horizontal|center_vertical"
        android:padding="2dp"
        android:paddingLeft="4dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:text="Call"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/locationBtnCov"
        app:layout_constraintHorizontal_bias="0.578"
        app:layout_constraintStart_toStartOf="@+id/locationBtnCov"
        app:layout_constraintTop_toTopOf="@+id/view2"
        app:drawableStartCompat="@drawable/ic_phone_call_button"
        app:drawableLeftCompat="@drawable/ic_phone_call_button"
        android:focusable="true" />

    <TextView
        android:id="@+id/locationBtnCov"
        android:layout_width="90dp"
        android:layout_height="28dp"
        android:background="@drawable/button_colour"
        android:drawableStart="@drawable/ic_location"
        android:drawablePadding="0dp"
        android:gravity="center_horizontal|center_vertical"
        android:padding="2dp"
        android:paddingLeft="4dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:text="Location"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/callBtnCov"
        app:layout_constraintVertical_bias="0.628"
        android:drawableLeft="@drawable/ic_location" />

</androidx.constraintlayout.widget.ConstraintLayout>