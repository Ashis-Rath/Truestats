<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginBottom="10dp"
    android:adjustViewBounds="false"
    android:paddingBottom="10dp">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="261dp"
        android:background="@drawable/box"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/nameOfOrgCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/segoeuibold"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Name of Organisation"
        android:textColor="#264157"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/addressOfOrgCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Address of Organisation"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/nameOfOrgCovTV"
        app:layout_constraintVertical_bias="0.045" />

    <View
        android:id="@+id/view2"
        android:layout_width="0.5dp"
        android:layout_height="150dp"
        android:background="#707070"
        app:layout_constraintBottom_toTopOf="@+id/updateDateTimeCovTV"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.627"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout7"
        app:layout_constraintVertical_bias="0.529" />

    <View
        android:id="@+id/view3"
        android:layout_width="75dp"
        android:layout_height="0.5dp"
        android:background="#707070"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="152dp"
        android:fontFamily="@font/sfprodisplayregular"
        android:text="ICU/Ventilator Beds:"
        android:textColor="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.218"
        app:layout_constraintStart_toStartOf="@+id/textView9" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="80dp"
        android:fontFamily="@font/sfprodisplayregular"
        android:text="Oxygen Beds:"
        android:textColor="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView14"
        app:layout_constraintHorizontal_bias="0.406"
        app:layout_constraintStart_toStartOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/totalVentiBedCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/total_bed_colour"
        android:fontFamily="@font/calibriregular"
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:text="Total: "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.091"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/textView14"
        app:layout_constraintVertical_bias="0.106" />

    <TextView
        android:id="@+id/totalOxyBedCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/total_bed_colour"
        android:fontFamily="@font/calibriregular"
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:text="Total: "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/totalVentiBedCovTV"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/totalVentiBedCovTV"
        app:layout_constraintTop_toBottomOf="@+id/textView15"
        app:layout_constraintVertical_bias="0.222" />

    <LinearLayout
        android:id="@+id/linearLayout7"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/textView14"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addressOfOrgCovTV"
        app:layout_constraintVertical_bias="0.333">

        <TextView
            android:id="@+id/cityOfOrgCovTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:hint="City"
            android:textSize="9sp" />

        <TextView
            android:id="@+id/stateOfOrgCovTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginLeft="10dp"
            android:hint="State"
            android:textSize="9sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/vacantVentiBedCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/calibriregular"
        android:text="Vacant: "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/totalVentiBedCovTV"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.456"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/totalVentiBedCovTV"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/vacantOxyBedCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/calibriregular"
        android:text="Vacant: "
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/totalOxyBedCovTV"
        app:layout_constraintEnd_toEndOf="@+id/vacantVentiBedCovTV"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/vacantVentiBedCovTV"
        app:layout_constraintTop_toTopOf="@+id/totalOxyBedCovTV"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/updateDateTimeCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:background="@drawable/box_date_time"
        android:fontFamily="@font/calibribold"
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:text="Last Updated: "
        android:textColor="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/vacantOxyBedCovTV"
        app:layout_constraintVertical_bias="0.913" />
    <TextView
        android:id="@+id/locationBtnCov"
        android:layout_width="90dp"
        android:layout_height="28dp"
        android:layout_marginBottom="6dp"
        android:background="@drawable/button_colour"
        android:drawableStart="@drawable/ic_location"
        android:drawableLeft="@drawable/ic_location"
        android:drawablePadding="0dp"
        android:gravity="center_horizontal|center_vertical"
        android:padding="2dp"
        android:paddingLeft="4dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:text="Location"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <TextView
        android:id="@+id/callBtnCov"
        android:layout_width="71dp"
        android:layout_height="28dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/button_colour"
        android:drawableStart="@drawable/ic_phone_call_button"
        android:drawableLeft="@drawable/ic_phone_call_button"
        android:drawablePadding="0dp"
        android:gravity="center_horizontal|center_vertical"
        android:padding="2dp"
        android:paddingLeft="4dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:text="Call"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2"
        app:layout_constraintVertical_bias="0.29" />

</androidx.constraintlayout.widget.ConstraintLayout>