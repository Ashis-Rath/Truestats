<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginBottom="10dp"
    android:adjustViewBounds="false"
    android:paddingBottom="10dp">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="261dp"
        android:background="@drawable/box"
        android:text=""
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.4" />

    <TextView
        android:id="@+id/nameOfOrgCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:fontFamily="@font/segoeuibold"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Name of Organisation"
        android:textColor="#264157"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toTopOf="@+id/textView9"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/addressOfOrgCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal|center_vertical"
        android:hint="Address of Organisation"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/nameOfOrgCovTV"
        app:layout_constraintVertical_bias="0.033" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addressOfOrgCovTV">

        <TextView
            android:id="@+id/stateOfOrgCovTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:hint="State"
            android:textColor="#000000"
            android:textSize="9sp" />

        <TextView
            android:id="@+id/cityOfOrgCovTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginLeft="30dp"
            android:gravity="right"
            android:hint="City"
            android:textColor="#000000"
            android:textSize="9sp" />
    </LinearLayout>

    <View
        android:id="@+id/view2"
        android:layout_width="0.5dp"
        android:layout_height="150dp"
        android:background="#707070"
        app:layout_constraintBottom_toTopOf="@+id/updateDateTimeCovTV"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.668"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addressOfOrgCovTV"
        app:layout_constraintVertical_bias="0.733" />

    <View
        android:id="@+id/view3"
        android:layout_width="75dp"
        android:layout_height="0.5dp"
        android:background="#707070"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <TextView
        android:id="@+id/totalVentiBedCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/total_bed_colour"
        android:fontFamily="@font/calibriregular"
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:text="Oxygen Availability Status"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toStartOf="@+id/view2"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/addressOfOrgCovTV"
        app:layout_constraintVertical_bias="0.24" />

    <TextView
        android:id="@+id/updateDateTimeCovTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:background="@drawable/box_date_time"
        android:fontFamily="@font/calibribold"
        android:paddingLeft="4dp"
        android:paddingRight="4dp"
        android:text="Last Updated: "
        android:textColor="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/vacantNormalBedTV" />

    <TextView
        android:id="@+id/statusUpdateOxiTV"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/segoeuibold"
        android:gravity="center"
        android:hint="Status"
        android:textColor="#000000"
        android:textSize="23sp"
        app:layout_constraintBottom_toTopOf="@+id/updateDateTimeCovTV"
        app:layout_constraintEnd_toStartOf="@+id/view2"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/totalVentiBedCovTV"
        app:layout_constraintVertical_bias="0.32" />

    <TextView
        android:id="@+id/callBtnCov"
        android:layout_width="71dp"
        android:layout_height="28dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/button_colour"
        android:drawableStart="@drawable/ic_phone_call_button"
        android:drawableLeft="@drawable/ic_phone_call_button"
        android:drawablePadding="0dp"
        android:gravity="center_horizontal|center_vertical"
        android:padding="2dp"
        android:paddingLeft="4dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:text="Call"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toTopOf="@+id/view2"
        app:layout_constraintVertical_bias="0.29" />

    <TextView
        android:id="@+id/locationBtnCov"
        android:layout_width="90dp"
        android:layout_height="28dp"
        android:layout_marginBottom="6dp"
        android:background="@drawable/button_colour"
        android:drawableStart="@drawable/ic_location"
        android:drawableLeft="@drawable/ic_location"
        android:drawablePadding="0dp"
        android:gravity="center_horizontal|center_vertical"
        android:padding="2dp"
        android:paddingLeft="4dp"
        android:paddingTop="3dp"
        android:paddingRight="6dp"
        android:paddingBottom="3dp"
        android:text="Location"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintEnd_toEndOf="@+id/textView9"
        app:layout_constraintStart_toEndOf="@+id/view2"
        app:layout_constraintTop_toBottomOf="@+id/view3" />


</androidx.constraintlayout.widget.ConstraintLayout>